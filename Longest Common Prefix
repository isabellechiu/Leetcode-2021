class Solution:
    def longestCommonPrefix(self, strs: List[str]) -> str:
        
        if len(strs) == 0:
            return ''

        short_str = min(strs,key=len)
        for i, char in enumerate(short_str):
            for a in strs:
                if a[i] != char:
                    return short_str[:i]
                
        return short_str 
        
